{
  "name": "taro-template",
  "author": "WangRuiJin <1361572192@qq.com>",
  "repository": "git@gitee.com:jh_shot/taro-template.git",
  "license": "MulanPSL2",
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "lint": "concurrently \"npm:lint:*\"",
    "lint:script": "eslint \"./src/**/*.{ts,vue}\" --fix",
    "lint:style": "stylelint \"./src/**/*.{css,scss,vue}\" --fix",
    "commit": "git add . && git cz",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@nutui/icons-vue-taro": "^0.0.9",
    "@nutui/nutui-taro": "4.0.8",
    "@tarojs/components": "3.6.7",
    "@tarojs/helper": "3.6.7",
    "@tarojs/plugin-framework-vue3": "3.6.7",
    "@tarojs/plugin-html": "3.6.7",
    "@tarojs/plugin-http": "3.6.7",
    "@tarojs/plugin-platform-alipay": "3.6.7",
    "@tarojs/plugin-platform-h5": "3.6.7",
    "@tarojs/plugin-platform-jd": "3.6.7",
    "@tarojs/plugin-platform-qq": "3.6.7",
    "@tarojs/plugin-platform-swan": "3.6.7",
    "@tarojs/plugin-platform-tt": "3.6.7",
    "@tarojs/plugin-platform-weapp": "3.6.7",
    "@tarojs/runtime": "3.6.7",
    "@tarojs/shared": "3.6.7",
    "@tarojs/taro": "3.6.7",
    "axios": "1.4.0",
    "pinia": "2.0.36",
    "vue": "3.2.47"
  },
  "devDependencies": {
    "@babel/core": "^7.13.0",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@tarojs/cli": "3.6.7",
    "@tarojs/webpack5-runner": "3.6.7",
    "@ttou/define-config": "^2.1.3",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "3.2.47",
    "@vue/eslint-config-typescript": "^11.0.3",
    "babel-preset-taro": "3.6.7",
    "commitizen": "^4.3.0",
    "concurrently": "^8.0.1",
    "css-loader": "6.8.1",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.13.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.8.1",
    "style-loader": "3.3.3",
    "stylelint": "^15.6.2",
    "stylelint-config-standard": "^33.0.0",
    "typescript": "^4.9.0",
    "unplugin-vue-components": "^0.24.1",
    "vue-loader": "^17.1.1",
    "webpack": "5.84.1"
  },
  "browserslist": [
    "last 3 versions",
    "Chrome >= 51",
    "Android >= 5",
    "ios >= 10"
  ],
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx commitlint --edit $1"
  }
}
