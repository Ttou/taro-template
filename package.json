{
  "name": "taro-template",
  "author": "WangRuiJin <1361572192@qq.com>",
  "repository": "git@gitee.com:jh_shot/taro-template.git",
  "license": "MulanPSL2",
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "lint:js": "eslint \"**/*.{ts,tsx,vue}\" --fix",
    "lint:css": "stylelint \"**/*.{css,vue}\" --fix",
    "commit": "git add . && git cz",
    "uvm": "taro update project",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@nutui/icons-vue-taro": "0.0.9",
    "@nutui/nutui-taro": "4.3.12",
    "@tarojs/components": "3.6.34",
    "@tarojs/helper": "3.6.34",
    "@tarojs/plugin-framework-vue3": "3.6.34",
    "@tarojs/plugin-html": "3.6.34",
    "@tarojs/plugin-http": "3.6.34",
    "@tarojs/plugin-platform-alipay": "3.6.34",
    "@tarojs/plugin-platform-h5": "3.6.34",
    "@tarojs/plugin-platform-jd": "3.6.34",
    "@tarojs/plugin-platform-qq": "3.6.34",
    "@tarojs/plugin-platform-swan": "3.6.34",
    "@tarojs/plugin-platform-tt": "3.6.34",
    "@tarojs/plugin-platform-weapp": "3.6.34",
    "@tarojs/runtime": "3.6.34",
    "@tarojs/shared": "3.6.34",
    "@tarojs/taro": "3.6.34",
    "axios": "1.7.3",
    "pinia": "2.2.0",
    "vue": "3.4.35",
    "vue-types": "5.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/runtime": "^7.24.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@nutui/auto-import-resolver": "^1.0.0",
    "@tarojs/cli": "3.6.34",
    "@tarojs/webpack5-runner": "3.6.34",
    "@types/webpack-env": "^1.18.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vue/babel-plugin-jsx": "^1.2.2",
    "@vue/compiler-sfc": "3.4.35",
    "@vue/eslint-config-typescript": "^13.0.0",
    "babel-preset-taro": "3.6.34",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^9.27.0",
    "lint-staged": "^15.2.8",
    "postcss": "^8.4.40",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^16.8.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-prettier": "^5.0.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.5",
    "unplugin-vue-components": "^0.27.3",
    "vue-loader": "^17.4.2",
    "webpack": "5.92.0"
  },
  "browserslist": [
    "Chrome >= 51",
    "ios >= 10"
  ],
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,vue}": [
      "stylelint --fix --allow-empty-input",
      "prettier --write"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx commitlint --edit $1"
  }
}
